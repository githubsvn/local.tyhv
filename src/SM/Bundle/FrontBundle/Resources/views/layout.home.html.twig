<!DOCTYPE html>
<!--[if IE 8]><html lang="en" class="no-js ie ie8"><![endif]-->
<!--[if IE 9]><html lang="en" class="no-js ie ie9"><![endif]-->
<!--[if gt IE 9]><!-->
<html lang="en" class="no-js">
  <!--<![endif]-->
  <head>
    {% render 'SMFrontBundle:Default:meta' %}
    <link rel="shortcut icon" href="{{ asset('web/front/images/favicon.ico') }}" type="image/x-icon">
    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('web/front/images/apple-touch-icon-57x57.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('web/front/images/apple-touch-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('web/front/images/apple-touch-icon-114x114.png') }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('web/front/images/apple-touch-icon-144x144.png') }}">
    <!-- build:css css/style.min.css-->
    <link rel="stylesheet" href="{{ asset('web/front/css/style.css') }}">
    <link rel="stylesheet" href="{{ asset('web/front/css/custom.css') }}">
    <!-- endbuild-->
    <!-- build:js js/modernizr.min.js-->
    <script src="{{ asset('web/front/js/modernizr.js') }}"></script>
    <!-- endbuild-->
  </head>
  <body>
    <div class="wrapper">
      <div class="inner">
        <div id="container">
          <header id="header">
            <!-- BEGIN HEADER -->
            {% render 'SMFrontBundle:Default:header' %}
            <!-- END HEADER -->
          </header>
          <div class="inner">
            <section class="homepage">
              <!-- BEGIN LEFT COLUMN-->
              <aside class="sidebar-left">
                <!-- BEGIN MENU LEFT -->
                {% render 'SMFrontBundle:Default:leftMenu' %}
                <!-- END MENU LEFT -->

                <!-- BEGIN GIA CA THI TRUONG -->
                {% render 'SMFrontBundle:Default:pricesMarket' %}
                <!-- END GIA CA THI TRUONG -->

                <!-- BEGIN SAN PHAM KHUYEN MAI-->
                {% render 'SMFrontBundle:Default:saleProduct' %}
                <!-- END SAN PHAM KHUYEN MAI-->

                <div class="block contact-block">
                  <!-- BEGIN SUPPORT-->
                  {% render 'SMFrontBundle:Default:support' %}
                  <!-- END SUPPORT-->

                  <!-- BEGIN REGISTER TO GET PRODUCT -->
                  {% render 'SMFrontBundle:Default:registerGetProduct' %}
                  <!-- END REGISTER TO GET PRODUCT -->
                </div>

                <!-- BEGIN COUNTER -->
                {% render 'SMFrontBundle:Default:counter' %}
                <!-- END COUNTER -->
            </aside>
            <!-- END LEFT COLUMN-->
  
              <section class="main-content">
                
                <!-- BEGIN SLIDE-->
                {% render 'SMFrontBundle:Default:slide' %}
                <!-- END SLIDE-->
                
                <section id="main">
                <!-- BEGIN BODY -->
                {% block body%}{% endblock %}
                <!-- END BODY -->
                
                <!-- BEGIN HOT NEWS -->
                  {% render 'SMFrontBundle:Default:company' %}
                  <!-- END HOT NEWS -->
                </section>
                
                <!-- BEGIN RIGHT -->
                <!-- END RIGHT -->
                <aside class="sidebar-right">
                  <!-- BEGIN VIDEO -->
                  {% render 'SMFrontBundle:Default:video' %}
                  <!-- END VIDEO -->
                    
                  <!-- BEGIN HOT NEWS -->
                  {% render 'SMFrontBundle:Default:hotNews' %}
                  <!-- END HOT NEWS -->
                  
                  <!-- BEGIN ADVERTISEMENT -->
                  {% render 'SMFrontBundle:Default:advertisement' %}
                  <!-- END ADVERTISEMENT -->
                  
                </aside>
                  
              </section>
            </section>
          </div>
        </div>
        {% render 'SMFrontBundle:Default:feedback' %}
        
        <!-- BEGIN FOOTER -->
        {% render 'SMFrontBundle:Default:footer' %}
        <!-- END FOOTER -->
        
      </div>
    </div>
      
    {% render 'SMFrontBundle:Default:leftAds' %}
    
    <!-- build:js js/libs.min.js-->
    <script src="{{ asset('web/front/js/libs.js') }}"></script>
    <!-- endbuild-->
    <!-- build:js js/l10n.min.js-->
    <script src="{{ asset('web/front/js/l10n.js') }}"></script>
    <!-- endbuild-->
    <!-- build:js js/plugins.min.js-->
    <script src="{{ asset('web/front/js/plugins.js') }}"></script>
    <script src="{{ asset('web/front/js/jquery.mousewheel.js') }}"></script>
    <script src="{{ asset('web/front/js/jquery.flexslider.js') }}"></script>
    <script src="{{ asset('web/front/js/bootstrap.js') }}"></script>
    <!-- endbuild-->
    <!-- build:js js/start.min.js-->
    <script src="{{ asset('web/front/js/start.js') }}"></script>
    <script src="{{ asset('web/front/js/custom.js') }}"></script>
    <!-- endbuild-->
  </body>
</html>